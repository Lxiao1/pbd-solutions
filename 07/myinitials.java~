public class myinitials {
	public static void main(String[] args) {
	System.out.println("L      X   X");
	System.out.println("L      X   X");
	System.out.println("L       X X");
	System.out.println("L        X");
	System.out.println("L       X X");
	System.out.println("L      X   X");
	System.out.println("LLLLL X     X");
}
}
